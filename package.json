{
  "name": "docker-composer",
  "scripts": {
    "patch-release": "npm version patch && npm publish && git push --follow-tags",
    "minor-release": "npm version minor && npm publish && git push --follow-tags",
    "major-release": "npm version major && npm publish && git push --follow-tags",
    "unlink": "yarn unlink @csnext/cs-client && yarn unlink @csnext/cs-core && yarn unlink @csnext/cs-gridstack",
    "cs": "yarn link @csnext/cs-client && yarn link @csnext/cs-core && yarn link @csnext/cs-gridstack",
    "dev": "cross-env NODE_ENV=development nodemon --watch webpack.config.js --exec \"webpack-dev-server\"",
    "build": "cross-env NODE_ENV=production webpack --progress --watch",
    "start": "concurrently \"yarn cs\" \"yarn dev\" \"cd ../../csnext/csnext && yarn dev\"",
    "all": "concurrently \"yarn cs\" \"yarn dev\" \"cd ../../csnext/csnext && yarn dev\""
  },
  "version": "0.0.1",
  "main": "index.js",
  "repository": "https://github.com/DRIVER-EU/docker-composer.git",
  "author": "erik.vullings <erik.vullings@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@csnext/cs-client": "^0.0.6",
    "@types/node": "^8.0.53",
    "axios": "^0.17.1",
    "lodash": "^4.17.4",
    "vue": "^2.5.13",
    "vue-class-component": "^6.1.2",
    "vue-property-decorator": "^6.0.0",
    "vuetify": "^v1.0.0-beta.4",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/yamljs": "^0.2.30",
    "awesome-typescript-loader": "^3.4.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.3.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "dc": "^2.1.9",
    "file-loader": "^1.1.6",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.19.1",
    "ts-loader": "^3.2.0",
    "typescript": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "vue-loader": "^13.7.0",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-dev-server": "^2.11.0"
  }
}
